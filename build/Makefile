
all:
	make -C core 
	make -C example1 
	make -C test1 
	make -C test2
	make -C timertest 
	make -C symtest 

clean:
	make -C core clean
	make -C example1 clean
	make -C test1 clean
	make -C test2 clean
	make -C timertest clean
	make -C symtest clean

depend:
	make -C core depend
	make -C example1 depend
	make -C test1 depend
	make -C test2 depend
	make -C timertest depend
	make -C symtest depend

